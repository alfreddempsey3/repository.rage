<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.pjd.playlist"
	name="DJing EDM Music Channels"
	version="1.5"
	provider-name="WHUFCLEE, VULCAN, PJD1976">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.python.koding.aio" version="0.5"/>
    <import addon="plugin.video.youtube" version="5.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">A collection of EDM music TV Channels</summary>
    <description lang="en">For more information visit https://www.djing.com/</description>
    <platform>all</platform>
    <forum>http://noobsandnerds.com/support</forum>
    <website>http://totalrevolution.tv</website>
    <source></source>
    <news>First release</news>
  </extension>
</addon>

<addon id="repository.rage" name="PJD Repo" version="0.0.2" provider-name="PJD1976">
	<extension point="xbmc.addon.repository" name="PJD Repo">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/alfreddempsey3/repository.rage/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/alfreddempsey3/repository.rage/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/alfreddempsey3/repository.rage/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Test repo</summary>
		<description lang="en">Test repo</description>
		<genre>livetv,music,uk,usa</genre>
	</extension>
</addon>
</addons>
